#include <stdio.h>

int main() {
    int i, j, fatorial = 1;

    for (i = 1; i <= 10; i++) {
        if (i % 2 != 0) {
        fatorial = 1;

        for (j = 1; j <= i; j++) {
            fatorial = fatorial * j;
        }
        
        printf("Fatorial de %d: %d.\n", i, fatorial);
        }
    }

    return 0;
}
