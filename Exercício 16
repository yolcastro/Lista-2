#include <stdio.h>

int main() {
    int i, tres = 0, cinco = 0;

    for (i = 0; i <= 200; i++)
    {
        if (i <= 100) {
            if (i % 3 == 0) {
                tres++;
            }  
        }

        if (i >= 100) {
            if (i % 5 == 0) {
                cinco++;
            }
        }
    }

    printf("Números divisíveis por 3 de 0 a 100: %d\n", tres);
    printf("Números divisíveis por 5 de 100 a 200: %d\n", cinco);

    return 0;
}
