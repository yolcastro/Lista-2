#include <stdio.h>

int main() {
    int num;
    
    printf("Digite um número:\n");
    scanf("%d", &num);

    int i, fatorial;
    fatorial = 1;
    
    for (i = 1; i < num + 1; i++){
        fatorial = fatorial * i;
    }
   
    printf("O fatorial de %d é %d\n", num, fatorial);

    return 0;
}
