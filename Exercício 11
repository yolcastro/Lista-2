#include <stdio.h>

int main() {
    int i, n, h;

    printf("Digite os limites da faixa:\n");
    scanf("%d %d", &i, &n);

    if (n >= i) {
        for (h = i; h <= n; h++)
        {
            if (h% 4 == 0) {
                printf("%d\n", h * h);
            }   
        }
    }
    else {
        for (h = n; h <= i; h++)
        {
            if (h % 4 == 0) {
                printf("%d\n", h * h);
            }   
        }
    }
    
    return 0;
}
